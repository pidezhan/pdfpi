<% if @upload.new_record? %>
  alert("Failed to upload file: <%= j @upload.errors.full_messages.join(', ').html_safe %>");
<% else %>
  $("#uploads").append("<%= j render(@upload) %>");
  $("#split_upload").html("<%= j render(@upload) %>");
	$("#split_upload > .row > .handle").hide();
  $("#stamp_upload").html("<%= j render(@upload) %>");
	$("#stamp_upload > .row > .handle").hide();
<% end %>